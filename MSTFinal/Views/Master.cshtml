<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>MST Fitness - Morecambe and Lancaster Nutrition, Coaching, Strength and Conditioning</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="MST Fitness - Lancaster and Morecambe's only cardio, nutrition, strength and conditioning gym facility"/>
    <meta name="keywords" content="gym, fitness, mst, morecambe, lancaster, cardio, strength, weights, weightlifting, powerlifting, olympic lifting, crossfit, personal trainer, coaching"/>
    <meta name="author" content="Harrison Fleetwood"/>

    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content=""/>
    <meta property="og:image" content=""/>
    <meta property="og:url" content=""/>
    <meta property="og:site_name" content=""/>
    <meta property="og:description" content=""/>
    <meta name="twitter:title" content=""/>
    <meta name="twitter:image" content=""/>
    <meta name="twitter:url" content=""/>
    <meta name="twitter:card" content=""/>

    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="shortcut icon" href="favicon.ico">

    <link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,700,900' rel='stylesheet' type='text/css'>

    <!-- Animate.css -->
    <link rel="stylesheet" href="~/css/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/css/icomoon.css">
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/css/bootstrap.css">
    <!-- Superfish -->
    <link rel="stylesheet" href="~/css/superfish.css">

    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/photoswipe.css">
    <link rel="stylesheet" href="~/css/blog-post.css">


    <!-- Modernizr JS -->
    <script src="~/js/modernizr-2.6.2.min.js"></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
        <script src="js/respond.min.js"></script>
    <![endif]-->

</head>
                       <body>
                           <div id="fh5co-wrapper">
                               <div id="fh5co-page">
                                   <div id="fh5co-header">
                                       <header id="fh5co-header-section">
                                           <div class="container">
                                               <div class="nav-header">
                                                   <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle"><i></i></a>
                                                   <h1 id="fh5co-logo">
                                                       <a href="index.html">MST <span>Fitness</span></a>
                                                   </h1>
                                                   <!-- START #fh5co-menu-wrap -->
                                                   <nav id="fh5co-menu-wrap" role="navigation">
                                                       <ul class="sf-menu" id="fh5co-primary-menu">
                                                           <li><a href="/">Home</a></li>
                                                           <li><a href="/results/">Results</a></li>
                                                           <li><a href="/about/">About</a></li>
                                                           <li><a href="/contact-page/">Contact</a></li>
                                                       </ul>
                                                   </nav>
                                               </div>
                                           </div>
                                       </header>
                                   </div>
                                   @RenderBody()
                               </div>
                           </div>
                           <script src="~/js/jquery.min.js"></script>
                           <!-- jQuery Easing -->
                           <script src="~/js/jquery.easing.1.3.js"></script>
                           <!-- Bootstrap -->
                           <script src="~/js/bootstrap.min.js"></script>
                           <!-- Waypoints -->
                           <script src="~/js/jquery.waypoints.min.js"></script>
                           <!-- Stellar -->
                           <script src="~/js/jquery.stellar.min.js"></script>
                           <!-- Superfish -->
                           <script src="~/js/hoverIntent.js"></script>
                           <script src="~/js/superfish.js"></script>
                           <script src="~/js/photoswipe-ui-default.min.js"></script>
                           <!-- Main JS (Do not remove) -->
                           <script src="~/js/main.js"></script>

                           <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCefOgb1ZWqYtj7raVSmN4PL2WkTrc-KyA&sensor=false"></script>
                           <script src="~/js/google_map.js"></script>


                           <footer>
                               <div id="footer">
                                   <div class="container">
                                       <div class="row">
                                           <div class="col-md-4 animate-box">
                                               <h3 class="section-title">About Us</h3>
                                               <p>Lancaster and Morecambe's only Cardio, Nutrition, Strength and Conditioning Facility. We offer personally tailored 6 month training and nutrition plans to help you reach your fitness goals.</p>
                                           </div>

                                           <div class="col-md-4 animate-box">
                                               <h3 class="section-title">Our Address</h3>
                                               <ul class="contact-info">
                                                   <li><i class="icon-map-marker"></i>MST Fitness, Westgate, Morecambe, LA3 1AB</li>
                                                   <li><i class="icon-phone"></i>+ 1234 5678 98</li>
                                                   <li><i class="icon-envelope"></i><a href="#">mstfitness@gmail.com</a></li>
                                               </ul>
                                           </div>
                                           <div class="col-md-4 animate-box">
                                               <h3 class="section-title">Enquiries</h3>
                                               @Html.Partial("FooterContactForm", new MSTFinal.Models.ContactModel())
                                           </div>
                                       </div>
                                       <div class="row copy-right">
                                           <div class="col-md-6 col-md-offset-3 text-center footer">
                                               <p class="fh5co-social-icons">
                                                   <a href="https://www.facebook.com/mstmorecambe"><i class="icon-facebook2"></i></a>
                                                   <a href="https://www.instagram.com/mstsystems/"><i class="icon-instagram"></i></a>
                                                   <a href="https://www.youtube.com/user/lolatshane"><i class="icon-youtube"></i></a>
                                               </p>
                                           </div>
                                       </div>
                                   </div>
                               </div>
                           </footer>
